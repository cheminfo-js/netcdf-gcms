  "bugs": {
    "url": "https://github.com/cheminfo/netcdf-gcms/issues"
  },
  "homepage": "https://github.com/cheminfo/netcdf-gcms#readme",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "cheminfo-tools": "^1.23.3",
    "eslint": "^8.28.0",
    "eslint-config-cheminfo": "^8.1.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.0"
  },
  "dependencies": {
    "netcdfjs": "^2.0.2"
